{
  "name": "medialog",
  "version": "1.0.0",
  "description": "Web application for tracking book reading and series/movie watching",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch"
  },
  "keywords": ["media", "log", "books", "series", "movies"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "server.js",
      "!node_modules/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  }
}
